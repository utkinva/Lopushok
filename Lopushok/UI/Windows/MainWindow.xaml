<Window x:Class="Lopushok.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lopushok"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1000" MinHeight="600" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="90"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Width="100" HorizontalAlignment="Center" VerticalAlignment="Top" Height="60"/>
        <Image HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="0" Width="Auto" Source="/UI/Logo/Лопушок.png"/>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" Name="MainFrame"/>
    </Grid>
</Window>
